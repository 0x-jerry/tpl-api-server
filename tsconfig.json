{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "paths": {
      "@prisma/client": [".prisma/client"],
      "@/*": ["src/*"]
    }
  },
  "exclude": ["node_modules"]
}
